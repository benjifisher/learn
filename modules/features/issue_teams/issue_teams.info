name = "Issue Teams"
description = "Pair up in teams to work on issues in the issue queue"
core = "7.x"
package = "Drupal Ladder"
version = "7.x-1.0"
project = "issue_teams"
dependencies[] = "ctools"
dependencies[] = "ladder"
dependencies[] = "lessons"
features[ctools][] = "ladder:default_ladder:1"
features[ctools][] = "lessons:default_lessons:1"
features[ladder_rung][] = "create_an_issue_team"
features[ladder_rung][] = "join_an_issue_team"
features[lessons_lesson][] = "create_an_issue_team"
features[lessons_lesson][] = "join_an_issue_team"
